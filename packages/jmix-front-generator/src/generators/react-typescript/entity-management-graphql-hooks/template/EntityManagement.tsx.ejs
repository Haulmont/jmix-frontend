import * as React from "react";
import <%= editComponentClass %> from "./<%= editComponentClass %>";
import <%= listComponentClass %> from "./<%= listComponentClass %>";
import {
  registerEntityEditorScreen,
  registerEntityBrowserScreen,
  registerRoute
} from "@haulmont/jmix-react-ui";

const ENTITY_NAME = '<%= entity.name %>';
const ROUTING_PATH = '/<%= nameLiteral %>';

registerRoute(
  `${ROUTING_PATH}/:entityId?`,
  ROUTING_PATH,
  '<%= nameLiteral %> list',
  <<%= listComponentClass %> />,
  ENTITY_NAME,
  '<%= className %>'
);
registerEntityEditorScreen(
  ENTITY_NAME,
  '<%= nameLiteral %>',
  <<%= editComponentClass %> />
);
registerEntityBrowserScreen(
  ENTITY_NAME,
  '<%= nameLiteral %>',
  <<%= listComponentClass %> />
);
