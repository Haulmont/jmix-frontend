import React from "react";
import <%= editorClassName %> from "./<%= editorClassName %>";
import <%= browserClassName %> from "./<%= browserClassName %>";
import { MasterDetailManager } from "@haulmont/jmix-react-antd";
import { registerEntityList } from '@haulmont/jmix-react-web';
import { observer } from "mobx-react";

const ENTITY_NAME = '<%= entity.name %>';
const ROUTING_PATH = '/<%= nameLiteral %>';

const <%= className %> = observer(() => {
  return (
    <MasterDetailManager
      editor={<<%= editorClassName %> />}
      browser={<<%= browserClassName %> />}
    />
  );
});

registerEntityList({
  component: <%= className %>,
  caption: 'screen.<%= className %>',
  screenId: '<%= className %>',
  entityName: ENTITY_NAME,
  menuOptions: {
    pathPattern: `${ROUTING_PATH}/:entityId?`,
    menuLink: ROUTING_PATH,
  }
});

export default <%= className %>;